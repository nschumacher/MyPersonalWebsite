$("#collapseBody").animate({"height" : "0"}, 250);
$('#collapseBody').promise().done(function(){
    document.getElementById('collapseBody').innerHTML = "";
    $("#collapseBody").animate({"height" : "500"}, 700);
    $("#collapseBody").html("<%= escape_javascript(render partial: 'inDevelopment', locals: { :remote => true } ) %>");
});
