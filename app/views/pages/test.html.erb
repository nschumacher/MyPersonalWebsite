<style>
	.navbar{
	  margin-bottom: 0px;
	  background: transparent;
	  font-family: 'Josefin Sans', sans-serif;
	}
    body {
	    padding: 50px 0px;
	}
	@media(max-width:935px) {
	    .navbar .navbar-form {
	        width: 185px;
	        padding-left:0;
	        padding-right:0;
	    }
	}
	@media(min-width:936px) {
	    .navbar .navbar-form {
	        width: 250px;
	    }
	}
	.navbar .navbar-form {
	  overflow-y: auto;
	    padding-top: 0;
	    padding-bottom: 0;
	    margin-right: 0;
	    margin-left: 0;
	}
	.navbar-inverse {
		border: none;
	}
	.nav li a {
	    position: relative;
	    display: block;
	    padding: 10px 0px;
	}
	@media (max-width: 1120px) {
	    .navbar-header {
	        float: none;
	    }
	    .navbar-toggle {
	        display: block;
	    }
	    .navbar-collapse {
	        border-top: 1px solid transparent;
	        box-shadow: inset 0 1px 0 rgba(255,255,255,0.1);
	    }
	    .navbar-collapse.collapse {
	        display: none!important;
	    }
	    .navbar-nav {
	        float: none!important;
	        margin: 7.5px -15px;
	    }
	    .navbar-nav>li {
	        float: none;
	    }
	    .navbar-nav>li>a {
	        padding-top: 10px;
	        padding-bottom: 10px;
	    }
	    .navbar-text {
	        float: none;
	        margin: 15px 0;
	    }
	    /* since 3.1.0 */
	    .navbar-collapse.collapse.in { 
	        display: block!important;
	    }
	    .collapsing {
	        overflow: hidden!important;
	    }
	}
</style>

<script>
$(document).ready(function () {
        $('.dropdown-toggle').dropdown();
    });
</script>



